    "cz-freecodecamp": "^1.0.0",
    "eslint": "^4.5.0",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "husky": "^0.14.3",
    "prettier": "^1.5.2",
    "validate-commit-msg": "^2.14.0"
  }
