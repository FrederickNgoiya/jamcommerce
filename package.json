    "commit": "git-cz",
    "commitmsg": "validate-commit-msg"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-freecodecamp"
    }
  },
  "author": "Berkeley Martinez <Berkeley@realworldreact.com>",
  "dependencies": {
    "classnames": "^2.2.5",
    "gatsby": "^1.0.1",
    "gatsby-link": "^1.0.1",
    "gatsby-plugin-react-helmet": "^1.0.1",
    "kouto-swiss": "^1.1.0",
    "prop-types": "^15.5.10",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-view-pager": "^0.5.1",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-create-types": "0.0.1",
    "reselect": "^3.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1"
  },
  "keywords": [
    "gatsby",
    "gocommerce",
    "react"
