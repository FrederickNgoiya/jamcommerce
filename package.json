{
  "name": "jamcommerce",
  "description": "",
  "version": "0.0.1",
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop --port 3000",
    "lint": "eslint --ext=js,jsx .",
    "lint:fix": "eslint . --ext=js,jsx --fix",
    "pretest": "npm run lint",
    "test": "echo 'yo'",
    "format": "prettier 'src/**/*.js' 'src/**/*.jsx' './gatsby*.js' './plugins/**/*.js' --trailing-comma all --single-quote --write",
